<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	@*	<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
	<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />*@

	<meta charset="UTF-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<title>@ViewData["Title"] - Behapy</title>
	<meta name="robots" content="noindex, follow" />
	<meta name="description" content="" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<!-- Favicon -->
	<link rel="shortcut icon" type="image/x-icon" href="~/images/favicon.ico" />

	<!-- CSS
	============================================ -->
	<!-- Vendor CSS (Contain Bootstrap, Icon Fonts) -->
	<link rel="stylesheet" href="~/css/shop/vendor/font-awesome.min.css" />
	<link rel="stylesheet" href="~/css/shop/vendor/Pe-icon-7-stroke.css" />

	<!-- Plugin CSS (Global Plugins Files) -->
	<link rel="stylesheet" href="~/css/shop/plugins/animate.min.css" />
	<link rel="stylesheet" href="~/css/shop/plugins/jquery-ui.min.css" />
	<link rel="stylesheet" href="~/css/shop/plugins/swiper-bundle.min.css" />
	<link rel="stylesheet" href="~/css/shop/plugins/nice-select.css" />
	<link rel="stylesheet" href="~/css/shop/plugins/magnific-popup.min.css" />

	<!-- Style CSS -->
	<link rel="stylesheet" href="~/css/shop/style.css" />
</head>
<body>
	<div class="main-wrapper">
		<partial name="_HeaderNavBar" />

		@RenderBody()

		<partial name="_Footer" />

		<a class="scroll-to-top" href="">
			<i class="fa fa-chevron-up"></i>
		</a>
	</div>

	<!-- Vendor JS -->
	<script src="~/js/shop/vendor/bootstrap.bundle.min.js"></script>
	<script src="~/js/shop/vendor/jquery-3.5.1.min.js"></script>
	<script src="~/js/shop/vendor/jquery-migrate-3.3.0.min.js"></script>
	<script src="~/js/shop/vendor/modernizr-3.11.2.min.js"></script>
	<script src="~/js/shop/vendor/jquery.waypoints.js"></script>

	<!--Plugins JS-->
	<script src="~/js/shop/plugins/wow.min.js"></script>
	<script src="~/js/shop/plugins/jquery-ui.min.js"></script>
	<script src="~/js/shop/plugins/swiper-bundle.min.js"></script>
	<script src="~/js/shop/plugins/jquery.nice-select.js"></script>
	<script src="~/js/shop/plugins/parallax.min.js"></script>
	<script src="~/js/shop/plugins/jquery.magnific-popup.min.js"></script>

	<!--Main JS (Common Activation Codes)-->
	<script src="~/js/shop/main.js"></script>

	@*<script src="~/lib/jquery/dist/jquery.min.js"></script>
	<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
	<script src="~/js/site.js" asp-append-version="true"></script>*@
	@await RenderSectionAsync("Scripts", required: false)
</body>
</html>
